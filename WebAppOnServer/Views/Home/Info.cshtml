
@{
    ViewData["Title"] = "Info";
}

<h2 style="color: #202122" center>Infomation</h2>
    <div style="color:midnightblue">
        <table cellpadding="2" cellspacing="2">
            <tr>
                <th>Claim</th>
                <th>Value</th>
            </tr>
            @foreach (var claim in User.Claims)
            {
                <tr>
                    <td>@claim.Type</td>
                    <td>@claim.Value</td>
                </tr>
            }
        </table>
    </div>

