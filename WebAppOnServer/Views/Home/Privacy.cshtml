@{
    ViewData["Title"] = "Privacy Policy";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

</head>
<body>
    <form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="Privacy">
        <div class="mb-3">
            <label for="formFileMultiple" class="form-label">@ViewData["Title"]</label>
            <input class="form-control" type="file" id="formFileMultiple" name="file" multiple>
        </div>
        <div class="d-grid gap-2">
            <input class="btn btn-primary" type="submit" value="Upload file">
            <span style="color:darkblue">@TempData["msg"]</span>
        </div>
    </form>
</body>
</html>
